<div class="container">
  <div class="row">
    <div class="col-9 offset-md-2">
      <strong><h4>ユーザー一覧</h4></strong>
      <table class="table">
        <thead>
          <tr>
            <th>アカウントネーム</th>
            <th>ユーザーネーム</th>
            <th>メールアドレス</th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td>
                <%= link_to admin_user_path(user.id) do %>
                  <%= user.account_name %>
                <%end%>  
              </td>
              <td><%= user.user_name %></td>
              <td><%= user.email %></td>
            </tr>
          <%end%>
        </tbody>
      </table>
    </div>
  </div>
</div>